<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <filter>
        <filter-name>TextFilter</filter-name>
        <filter-class>club.huangliang.filter.TextFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>TextFilter</filter-name>

        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ChickImgServlet</servlet-name>
        <servlet-class>club.huangliang.servlet.ChickImgServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>ChickImgServlet</servlet-name>
        <url-pattern>/ChickImgServlet</url-pattern>
    </servlet-mapping>
